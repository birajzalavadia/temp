<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content"
	android:layout_height="fill_parent"
	android:background="@android:color/white"
	android:baselineAligned="true"
	android:orientation="vertical"
	android:weightSum="1"
	android:gravity="center_vertical">

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@android:color/white"
		android:orientation="vertical"
		android:weightSum="1">

		<ImageView
			android:id="@+id/tab1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_weight="0.20"
			android:src="@drawable/tab_item_land_1" />

		<ImageView
			android:id="@+id/tab2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_weight="0.20"
			android:src="@drawable/tab_item_land_2" />

		<ImageView
			android:id="@+id/tab3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_weight="0.20"
			android:src="@drawable/tab_item_land_3" />


		<ImageView
			android:id="@+id/tab4"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_weight="0.20"
			android:src="@drawable/tab_item_land_4" />

		<ImageView
			android:id="@+id/tab5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_gravity="bottom"
			android:layout_weight="0.20"
			android:src="@drawable/tab_item_land_5" />

	</LinearLayout>

</LinearLayout>